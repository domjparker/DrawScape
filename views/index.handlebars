{{!-- materialize sign up  --}}
<div class="row">
  
  <br>
  <br>
  <form id="sign-up-container" class="col s12">
    <div class="container">
      <h4>Sign Up!</h4>
      <div class="row">
        <div class="input-field col s6 center">
          <input id="userInput" type="text" class="validate">
          <label for="first_name">First Name</label>
        </div>
      </div>
      <div class="row">
        <div class="input-field col s6 center">
          <input id="emailInput" type="email" class="validate">
          <label for="email">Enter Email</label>
        </div>
      </div>
      <div class="row">
        <div class="input-field col s6 center">
          <input id="passInput" type="password" class="validate">
          <label for="password">Create Password</label>
        </div>
      </div>
      <button class="button" id="user-send-button">Go <i class="fa fa-pencil" style="font-size:14px"></i></button>
    </div>

  </form>
</div>





