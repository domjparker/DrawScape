<form id="log-in-container">
  <div class="container">
    <h4>Log In</h4>
    <div class="row">
      <div class="input-field col s6 center">
        <input id="emailLogInput" type="text" class="validate">
        <label for="first_name">Email</label>
      </div>
    </div>
    <div class="row">
      <div class="input-field col s6 center">
        <input id="passLogInput" type="text" class="validate">
        <label for="first_name">Password</label>
      </div>
    </div>
    <button class="button" id="user-send-button">Go <i class="fa fa-pencil" style="font-size:14px"></i></button>
  </div>
</form>
