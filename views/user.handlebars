<body class="bodyUser">

  {{!-- navbar --}}
  <nav>
    <div class="nav-wrapper blue lighten-2">
      <a href="#home" class="brand-logo center black-text">User Profile</a>
      <ul class="left hide-on-med-and-down">
        <li><a href="#home"><i class="material-icons">home</i></a></li>
      </ul>
    </div>
  </nav>

  <div class="row">
    <div class="container">
      <div class="col m6 l6">

        {{!-- user details section card --}}
        <div class="card mt5">
          <div class="card-content white-text">
            <span class="card-title grey-text text-darken-3">User:</span>
            {{#each userDetails}}
            <h5>Username: {{user_name}}</h5>
            <h5>email: {{email}}</h5>
            <h5>password: {{password}}</h5>
            {{/each}}
          </div>
          <div class="card-reveal">
            <span class="card-title grey-text text-darken-3">Card Title<i class="material-icons right">close</i></span>
            <h5>Update Details</h5>
          </div>
        </div>

        <div class="card mt5">
          
          <div class="col m6 l6">
            <ul class="collection with-header mt5">
              <li class="collection-header">
                <h4>Rooms you're in:</h4>
              </li>
              {{#each rooms}}
              <li class="collection-item">
                <div>Alvin<a href="#!" class="secondary-content"><i class="material-icons">send</i></a></div>
              </li>
              {{/each}}
            </ul>
          </div>
        </div>

      </div>
    </div>
  </div>



</body>

<script src="/assets/js/user.js"></script>